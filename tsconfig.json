{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "es2020",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"baseUrl": "."
	},
	"include": ["src/**/*.ts", "./.eslintrc.js"],
	"exclude": ["node_modules"]
}
